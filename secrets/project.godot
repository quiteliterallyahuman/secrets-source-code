config_version=5

[application]

config/name="secrets"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://textures/icon.png"
config/icon="res://textures/icon.png"
config/windows_native_icon="res://icon.ico"
boot_splash/minimum_display_time=20

[autoload]

Tools="*res://scripts/tools.gd"

[display]

window/size/viewport_width=448
window/size/viewport_height=448
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/handheld/orientation=4

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/movie_file="E:/godot/recorded/secrets.avi"

[file_customization]

folder_colors={
"res://scenes/": "red",
"res://scripts/": "purple",
"res://sound/": "pink",
"res://textures/": "orange"
}

